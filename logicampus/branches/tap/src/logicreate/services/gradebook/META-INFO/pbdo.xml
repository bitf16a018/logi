<?xml version="1.0"?>
<!-- MySQL does not support foreign keys. This means   -->
<!-- that this XML file will not reflect foreign key   -->
<!-- relationships. You will have to do that yourself. -->
<database name="logicampus">
  <table  name="class_gradebook">
    <column name="id_class_gradebook"
            required="true"
            primaryKey="true"
            size="10"
            type="INTEGER"/>
    <column name="id_classes"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <column name="a_upper"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
    <column name="a_lower"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
    <column name="b_lower"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
    <column name="c_lower"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
    <column name="d_lower"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
    <column name="calculation_type"
            required="true"
            default="0"
            size="1"
            type="INTEGER"/>
    <column name="color_missing_grade"
            required="true"
            default="FFC2CD"
            size="10"
            type="CHAR"/>
    <column name="roundScoresUp"
            required="true"
            default="0"
            size="1"
            type="TINYINT"/>
    <column name="total_points"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <unique>
      <unique-column name="id_classes"/>
    </unique>
  </table>
  <table  name="class_gradebook_categories">
    <column name="id_class_gradebook_categories"
            required="true"
            primaryKey="true"
            size="10"
            type="INTEGER"/>
    <column name="id_classes"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <column name="label"
            required="true"
            size="255"
            type="VARCHAR"/>
    <column name="weight"
            required="false"
            size=""
            type="FLOAT"/>
    <index>
      <index-column name="id_classes"/>
    </index>
  </table>
  <table  name="class_gradebook_entries">
    <column name="id_class_gradebook_entries"
            required="true"
            primaryKey="true"
            size="10"
            type="INTEGER"/>
    <column name="id_classes"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <column name="id_class_gradebook_categories"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <column name="title"
            required="true"
            size="100"
            type="VARCHAR"/>
    <column name="gradebook_code"
            required="true"
            size="100"
            type="VARCHAR"/>
    <column name="total_points"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
    <column name="publish_flag"
            required="true"
            default="0"
            size="1"
            type="TINYINT"/>
    <column name="date_due"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <column name="notes"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <column name="assessment_id"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="assignment_id"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <index>
      <index-column name="id_classes"/>
      <index-column name="id_class_gradebook_categories"/>
      <index-column name="date_due"/>
      <index-column name="assignment_id"/>
    </index>
  </table>
  <table  name="class_gradebook_val">
    <column name="id_class_gradebook_val"
            required="true"
            primaryKey="true"
            size="10"
            type="INTEGER"/>
    <column name="id_class_gradebook_entries"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="id_classes"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <column name="username"
            required="true"
            size="30"
            type="VARCHAR"/>
    <column name="score"
            required="false"
            size=""
            type="FLOAT"/>
    <column name="comments"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <column name="date_created"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <column name="date_modified"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <index>
      <index-column name="id_class_gradebook_entries"/>
      <index-column name="id_classes"/>
      <index-column name="username"/>
      <index-column name="date_modified"/>
    </index>
  </table>
</database>
