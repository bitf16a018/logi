<?xml version="1.0"?>
<!-- MySQL does not support foreign keys. This means   -->
<!-- that this XML file will not reflect foreign key   -->
<!-- relationships. You will have to do that yourself. -->
<database name="logicampus">
  <table  name="textbook">
    <column name="id_textbook"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <column name="id_classes"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <column name="south_campus"
            required="false"
            default="0"
            size="1"
            type="INTEGER"/>
    <column name="southeast_campus"
            required="false"
            default="0"
            size="1"
            type="INTEGER"/>
    <column name="northeast_campus"
            required="false"
            default="0"
            size="1"
            type="INTEGER"/>
    <column name="northwest_campus"
            required="false"
            default="0"
            size="1"
            type="INTEGER"/>
    <column name="noTextbooks"
            required="true"
            default="0"
            size="1"
            type="TINYINT"/>
    <unique>
      <unique-column name="id_classes"/>
    </unique>
  </table>
  <table  name="textbook_classes">
    <column name="id_textbook_classes"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <column name="id_classes"
            required="true"
            default="0"
            size="11"
            type="BIGINT"/>
    <column name="author"
            required="true"
            size="100"
            type="VARCHAR"/>
    <column name="title"
            required="true"
            size="255"
            type="VARCHAR"/>
    <column name="publisher"
            required="true"
            size="255"
            type="VARCHAR"/>
    <column name="edition"
            required="false"
            size="255"
            type="VARCHAR"/>
    <column name="copyright"
            required="false"
            size="4"
            type="YEAR"/>
    <column name="isbn"
            required="true"
            size="25"
            type="VARCHAR"/>
    <column name="required"
            required="false"
            default="0"
            size="1"
            type="INTEGER"/>
    <column name="bundled"
            required="false"
            default="0"
            size="1"
            type="INTEGER"/>
    <column name="bundled_items"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <column name="type"
            required="true"
            size="25"
            type="VARCHAR"/>
    <column name="status"
            required="true"
            default="1"
            size="1"
            type="TINYINT"/>
    <column name="note"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <index>
      <index-column name="id_classes"/>
    </index>
  </table>
  <table  name="textbook_estimates">
    <column name="textbook_estimates_key"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <column name="textbook_estimates_name"
            required="true"
            size="30"
            type="VARCHAR"/>
  </table>
  <table  name="textbook_estimates_data">
    <column name="textbook_estimates_data_key"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <column name="textbook_estimates_key"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="id_original_textbook_classes"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <column name="bundled"
            required="true"
            default="0"
            size="4"
            type="TINYINT"/>
    <column name="courseFamily"
            required="true"
            size="20"
            type="VARCHAR"/>
    <column name="courseNumber"
            required="true"
            size="10"
            type="VARCHAR"/>
    <column name="isbn"
            required="true"
            size="50"
            type="VARCHAR"/>
    <column name="edition"
            required="true"
            size="50"
            type="VARCHAR"/>
    <column name="title"
            required="true"
            size="100"
            type="VARCHAR"/>
    <column name="author"
            required="true"
            size="100"
            type="VARCHAR"/>
    <column name="type"
            required="true"
            size="20"
            type="VARCHAR"/>
    <column name="se"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="so"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="nw"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="ne"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="note"
            required="true"
            size=""
            type="LONGVARCHAR"/>
    <column name="instructor"
            required="true"
            size="100"
            type="VARCHAR"/>
  </table>
</database>
