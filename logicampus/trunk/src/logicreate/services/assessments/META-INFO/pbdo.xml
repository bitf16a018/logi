<?xml version="1.0"?>
<!-- MySQL does not support foreign keys. This means   -->
<!-- that this XML file will not reflect foreign key   -->
<!-- relationships. You will have to do that yourself. -->
<database name="logicampus">
  <table  name="assessment">
    <column name="assessment_id"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <column name="display_name"
            required="false"
            size="255"
            type="VARCHAR"/>
    <column name="class_id"
            required="false"
            size="11"
            type="INTEGER"/>
    <column name="date_available"
            required="false"
            size="14"
            type="INTEGER"/>
    <column name="date_unavailable"
            required="false"
            size="11"
            type="INTEGER"/>
    <column name="mail_responses"
            required="false"
            size="1"
            type="TINYINT"/>
    <column name="auto_publish"
            required="false"
            size="1"
            type="TINYINT"/>
    <column name="num_retries"
            required="false"
            size="4"
            type="TINYINT"/>
    <column name="minute_limit"
            required="false"
            size="11"
            type="INTEGER"/>
    <column name="description"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <column name="instructions"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <column name="show_result_type"
            required="true"
            default="0"
            size="1"
            type="TINYINT"/>
    <column name="possible_points"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
  </table>
  <table  name="assessment_answer">
    <column name="assessment_answer_id"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <column name="assessment_id"
            required="false"
            size="11"
            type="INTEGER"/>
    <column name="assessment_question_id"
            required="false"
            size="11"
            type="INTEGER"/>
    <column name="student_id"
            required="false"
            size="32"
            type="VARCHAR"/>
    <column name="assessment_answer_values"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <column name="id_classes"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="points_earned"
            required="false"
            size=""
            type="FLOAT"/>
    <column name="points_given"
            required="false"
            size=""
            type="FLOAT"/>
    <index>
      <index-column name="assessment_id"/>
      <index-column name="assessment_question_id"/>
      <index-column name="student_id"/>
      <index-column name="id_classes"/>
    </index>
  </table>
  <table  name="assessment_log">
    <column name="id_assessment_log"
            required="true"
            primaryKey="true"
            size="10"
            type="INTEGER"/>
    <column name="assessment_id"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="id_student"
            required="true"
            size="32"
            type="VARCHAR"/>
    <column name="start_date"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="end_date"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <column name="id_classes"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <index>
      <index-column name="assessment_id"/>
    </index>
  </table>
  <table  name="assessment_question">
    <column name="assessment_question_id"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <column name="assessment_id"
            required="false"
            size="11"
            type="INTEGER"/>
    <column name="question_type"
            required="false"
            size="11"
            type="INTEGER"/>
    <column name="question_sort"
            required="true"
            default="0"
            size="4"
            type="TINYINT"/>
    <column name="question_points"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
    <column name="question_display"
            required="false"
            size="255"
            type="VARCHAR"/>
    <column name="question_text"
            required="false"
            size=""
            type="MEDIUMTEXT"/>
    <column name="question_choices"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <column name="question_input"
            required="true"
            size=""
            type="LONGVARCHAR"/>
    <column name="file_hash"
            required="true"
            size="32"
            type="VARCHAR"/>
  </table>
</database>
