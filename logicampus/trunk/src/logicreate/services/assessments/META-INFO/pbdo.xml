<?xml version="1.0"?>
<project name="logicampus">
  <entity  name="assessment">
    <attribute name="assessment_id"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <attribute name="display_name"
            required="false"
            size="255"
            type="VARCHAR"/>
    <attribute name="class_id"
            required="false"
            size="11"
            type="INTEGER"/>
    <attribute name="date_available"
            required="false"
            size="14"
            type="INTEGER"/>
    <attribute name="date_unavailable"
            required="false"
            size="11"
            type="INTEGER"/>
    <attribute name="mail_responses"
            required="false"
            size="1"
            type="TINYINT"/>
    <attribute name="auto_publish"
            required="false"
            size="1"
            type="TINYINT"/>
    <attribute name="num_retries"
            required="false"
            size="4"
            type="TINYINT"/>
    <attribute name="minute_limit"
            required="false"
            size="11"
            type="INTEGER"/>
    <attribute name="description"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <attribute name="instructions"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <attribute name="show_result_type"
            required="true"
            default="0"
            size="1"
            type="TINYINT"/>
    <attribute name="possible_points"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
  </entity>
  <entity  name="assessment_answer">
    <attribute name="assessment_answer_id"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <attribute name="assessment_id"
            required="false"
            size="11"
            type="INTEGER"/>
    <attribute name="assessment_question_id"
            required="false"
            size="11"
            type="INTEGER"/>
    <attribute name="student_id"
            required="false"
            size="32"
            type="VARCHAR"/>
    <attribute name="assessment_answer_values"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <attribute name="id_classes"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <attribute name="points_earned"
            required="false"
            size=""
            type="FLOAT"/>
    <attribute name="points_given"
            required="false"
            size=""
            type="FLOAT"/>
    <index>
      <index-column name="assessment_id"/>
      <index-column name="assessment_question_id"/>
      <index-column name="student_id"/>
      <index-column name="id_classes"/>
    </index>
  </entity>
  <entity  name="assessment_log">
    <attribute name="id_assessment_log"
            required="true"
            primaryKey="true"
            size="10"
            type="INTEGER"/>
    <attribute name="assessment_id"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <attribute name="id_student"
            required="true"
            size="32"
            type="VARCHAR"/>
    <attribute name="start_date"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <attribute name="end_date"
            required="true"
            default="0"
            size="11"
            type="INTEGER"/>
    <attribute name="id_classes"
            required="true"
            default="0"
            size="10"
            type="INTEGER"/>
    <index>
      <index-column name="assessment_id"/>
    </index>
  </entity>
  <entity  name="assessment_question">
    <attribute name="assessment_question_id"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <attribute name="assessment_id"
            required="false"
            size="11"
            type="INTEGER"/>
    <attribute name="question_type"
            required="false"
            size="11"
            type="INTEGER"/>
    <attribute name="question_sort"
            required="true"
            default="0"
            size="4"
            type="TINYINT"/>
    <attribute name="question_points"
            required="true"
            default="0"
            size=""
            type="FLOAT"/>
    <attribute name="question_display"
            required="false"
            size="255"
            type="VARCHAR"/>
    <attribute name="question_text"
            required="false"
            size=""
            type="MEDIUMTEXT"/>
    <attribute name="question_choices"
            required="false"
            size=""
            type="LONGVARCHAR"/>
    <attribute name="question_input"
            required="true"
            size=""
            type="LONGVARCHAR"/>
    <attribute name="file_hash"
            required="true"
            size="32"
            type="VARCHAR"/>
  </entity>
  <entity  name="assessment_grade">
    <attribute name="assessment_grade_id"
            required="true"
            primaryKey="true"
            size="11"
            type="INTEGER"/>
    <attribute name="assessment_id"
            required="false"
            size="11"
            type="INTEGER"/>
    <attribute name="student_id"
            required="true"
            size="32"
            type="VARCHAR"/>
    <attribute name="comments"
            required="true"
            size=""
            type="LONGVARCHAR"/>
    <attribute name="points"
            required="false"
            size="10,2"
            type="FLOAT"/>
    <attribute name="points_override"
            required="false"
            size="10,2"
            type="FLOAT"/>
  </entity>
	<entity name="assessment_event_link">
		<attribute
			name = "assessment_event_link_id"
			type = "int"
			size = "10"
			primaryKey  = "true" />
		<attribute
			name = "assessment_id"
			type = "int"
			size = "10" />
		<attribute
			name = "lc_event_id"
			type = "int"
			size = "10" />
		<key
			name = "assessment_idx"
			attribute="assessment_id"/>
		<key
			name = "lc_event_idx"
			attribute="lc_event_id"/>
	</entity>
</project>
